type User {
  id: Int!
  first_name: String!
  last_name: String!
  email: String!
  phone_number: String!
  password: String!
  token: String!
}

input NewUser {
  first_name: String!
  last_name: String!
  email: String!
  phone_number: String!
  password: String!
}

input LoginInput {
  login_text: String!
  password: String!
}

type Query {
  users: [User!]! 
}

type Mutation {
  Register(input: NewUser!): User!
  Login(input: LoginInput!): User!
}
